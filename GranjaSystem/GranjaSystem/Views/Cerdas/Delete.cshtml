@model GranjaSystem.Models.tblCerdas

@{
    ViewBag.Title = "Eliminar Registro";
}

    <div class="row content container-fluid">
        <div class="panel panel-primary">
            <div class="panel-heading text-center"><h4>CONTROL DE CERDA  N° @Html.DisplayFor(model => model.NumCerda)</h4></div>
            <div class="panel-body bg-gray">
                <h5 class="alert alert-danger">¿Estas Seguro de Eliminar el Registro?</h5>
                <div>
                    <div class="col-sm-6">
                        <dl class="dl-horizontal">
                            <dt>
                                Genética
                            </dt>

                            <dd>
                                @Html.DisplayFor(model => model.tblGeneticas.Genetica)
                            </dd>

                            <dt>
                                @Html.DisplayNameFor(model => model.Procedencia)
                            </dt>

                            <dd>
                                @Html.DisplayFor(model => model.Procedencia)
                            </dd>

                            <dt>
                                @Html.DisplayNameFor(model => model.Observaciones)
                            </dt>

                            <dd>
                                @Html.DisplayFor(model => model.Observaciones)
                            </dd>
                        </dl>
                    </div>
                    <div class="col-sm-6">
                        <dl class="dl-horizontal">
                            <dt>
                                Número de Partos
                            </dt>

                            <dd>
                                @Html.DisplayFor(model => model.NumParto)
                            </dd>

                            <dt>
                                Fecha De Registro
                            </dt>

                            <dd>
                                @Html.DisplayFor(model => model.FechaRegistro)
                            </dd>

                            <dt>
                                @Html.DisplayNameFor(model => model.Estado)
                            </dt>

                            <dd>
                                @Html.DisplayFor(model => model.Estado)
                            </dd>
                        </dl>
                    </div>

                    @using (Html.BeginForm())
                    {
                        @Html.AntiForgeryToken()

                        <div class="col-sm-offset-9 col-sm-3" align="right">
                            <input type="submit" value="Eliminar" class="btn btn-danger" />
                            @Html.ActionLink("Atras", "Index", null, new { @class = "btn btn-info" })
                        </div>
                    }
                </div>
            </div>
        </div>
    </div>

