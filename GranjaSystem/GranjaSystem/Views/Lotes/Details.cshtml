            <div class="panel-heading text-center"><h4>CONTROL DE LOTES</h4></div>
            <div class="panel-body bg-gray">

                <div>

                    <hr />
                    <dl class="dl-horizontal">
                        <dt>
                            @Html.DisplayNameFor(model => model.NumLote)
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.NumLote)
                        </dd>

                        <dt>
                            @Html.DisplayNameFor(model => model.FechaRegistro)
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.FechaRegistro)
                        </dd>

