@model GranjaSystem.Models.tblVarracos

@{
    ViewBag.Title = "Detalles del Registro";
}
    <div class="row content container-fluid">
        <div class="panel panel-primary">
            <div class="panel-heading text-center"><h4>CONTROL DE VARRACOS</h4></div>
            <div class="panel-body bg-gray">
                <div style="font-size: 20px;">
                    <dl class="dl-horizontal">
                        <dt>
                            Genética
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.tblGeneticas.Genetica)
                        </dd>

                        <dt>
                            Varraco
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.NumVarraco)
                        </dd>

                        <dt>
                            @Html.DisplayNameFor(model => model.Procedencia)
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.Procedencia)
                        </dd>

                        <dt>
                            @Html.DisplayNameFor(model => model.Observaciones)
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.Observaciones)
                        </dd>

                        <dt>
                            Fecha Registro
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.FechaRegistro)
                        </dd>

                        <dt>
                            @Html.DisplayNameFor(model => model.Estado)
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.Estado)
                        </dd>

                    </dl>
                </div>
                <div class="col-sm-offset-9 col-sm-3" align="right">
                    @Html.ActionLink("Modificar", "Edit", new { id = Model.IdVarraco }, new { @class = "btn btn-primary" })
                    @Html.ActionLink("Atras", "Index", null, new { @class = "btn btn-success" })
                </div>
            </div>
        </div>
    </div>

