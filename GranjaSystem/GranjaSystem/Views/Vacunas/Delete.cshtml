@model GranjaSystem.Models.tblVacunas

@{
    ViewBag.Title = "Eliminar Registro";
}
    <div class="row content container-fluid">
        <div class="panel panel-primary">
            <div class="panel-heading text-center"><h4>CONTROL DE VACUNAS</h4></div>
            <div class="panel-body bg-gray">
                <h5 class="alert alert-danger">¿Estas Seguro de Eliminar el Registro?</h5>
                <div>
                    <dl class="dl-horizontal">
                        <dt>
                            @Html.DisplayNameFor(model => model.Cerdas.Procedencia)
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.Cerdas.Procedencia)
                        </dd>

                        <dt>
                            Fecha Registro
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.FechaRegistro)
                        </dd>

                        <dt>
                            Fecha Inyección  
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.FechaInyeccion)
                        </dd>

                        <dt>
                            @Html.DisplayNameFor(model => model.Vacuna)
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.Vacuna)
                        </dd>

                        <dt>
                            Descripción
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.Descripcion)
                        </dd>

                    </dl>

                    @using (Html.BeginForm())
                    {
                        @Html.AntiForgeryToken()

                <div class="col-sm-offset-9 col-sm-3" align="right">
                    <input type="submit" value="Eliminar" class="btn btn-danger" />
                    @Html.ActionLink("Atras", "Index", null, new { @class = "btn btn-info" })
                </div>
                    }
                </div>
            </div>
        </div>
    </div>

