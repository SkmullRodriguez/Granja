@model GranjaSystem.Models.tblVacunas

@{
    ViewBag.Title = "Detalles del Registro";
}
    <div class="row content container-fluid">
        <div class="panel panel-primary">
            <div class="panel-heading text-center"><h4>CONTROL DE VACUNAS</h4></div>
            <div class="panel-body bg-gray">
                <div>
                    <dl class="dl-horizontal">
                        <dt>
                            @Html.DisplayNameFor(model => model.Cerdas.Procedencia)
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.Cerdas.Procedencia)
                        </dd>

                        <dt>
                            Fecha Registro
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.FechaRegistro)
                        </dd>

                        <dt>
                            Fecha Inyección
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.FechaInyeccion)
                        </dd>

                        <dt>
                            @Html.DisplayNameFor(model => model.Vacuna)
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.Vacuna)
                        </dd>

                        <dt>
                            Descripción
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.Descripcion)
                        </dd>

                    </dl>
                </div>
                <p align="right">
                    @Html.ActionLink("Modificar", "Edit", new { id = Model.IdVacuna }, new { @class = "btn btn-primary" }) |
                    @Html.ActionLink("Atras", "Index", null, new { @class = "btn btn-success" })
                </p>
            </div>
        </div>
     
    </div>

